            "args": [ "download", "did:plc:urj4pkche2yvpr5ilj4ykteu", "bafkreif2su5dt27u23q2f4tl5mgmojyibl4rj6lundc27sjvtuhawi732y", "-o", "${workspaceFolder}/artifacts/avatar.jpg" ],
