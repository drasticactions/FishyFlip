{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "OAuth Start",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_oauth",
            "program": "${workspaceFolder}/samples/OAuth/bin/Debug/net9.0/OAuth.dll",
            "args": [ "start", "-id", "drasticactions.dev" ],
            "cwd": "${workspaceFolder}/samples/OAuth",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "FFSourceGen",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_ffsourcegen",
            "program": "${workspaceFolder}/tools/FFSourceGen/bin/Debug/net9.0/FFSourceGen.dll",
            "args": [ "generate", "${workspaceFolder}/../fflexicons/bluesky-social-atproto/lexicons", "-o", "${workspaceFolder}/src/FishyFlip/" ],
            "cwd": "${workspaceFolder}/tools/FFSourceGen",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}